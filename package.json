{
  "name": "real-estate-project",
  "version": "1.0.0",
  "description": "A modern, full-featured real estate management system built with the MERN stack",
  "main": "index.js",
  "scripts": {
    "install": "npm run install:backend && npm run install:frontend",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm start",
    "build:frontend": "cd frontend && npm run build",
    "start": "cd backend && npm start"
  },
  "keywords": [
    "real-estate",
    "mern-stack",
    "react",
    "nodejs",
    "express",
    "mongodb",
    "admin-dashboard"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/real-estate-project.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/real-estate-project/issues"
  },
  "homepage": "https://github.com/yourusername/real-estate-project#readme"
}
